{
    "workbench.iconTheme": "material-icon-theme",
    "files.autoSave": "afterDelay",
    "editor.wordWrap": "off",
    "vsicons.dontShowNewVersionMessage": true,
    "solargraph.diagnostics": true,
    "workbench.editor.showTabs": false,
    "editor.minimap.enabled": false,
    "workbench.activityBar.visible": false,
    "editor.renderWhitespace": "none",
    "explorer.openEditors.visible": 0,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "terminal.external.osxExec": "Alacritty.app",
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "terminal.integrated.scrollback": 5000,
    "workbench.colorCustomizations": {
        "terminal.background": "#37364a",
        "terminal.foreground": "#d0d2d1",
        "terminalCursor.background": "#27292c",
        "terminalCursor.foreground": "#d0d2d1",
        "terminal.ansiBlack": "#003541",
        "terminal.ansiBlue": "#93b2ca",
        "terminal.ansiBrightBlack": "#666666",
        "terminal.ansiBrightBlue": "#93b2ca",
        "terminal.ansiBrightCyan": "#9ac9c4",
        "terminal.ansiBrightGreen": "#c2c77b",
        "terminal.ansiBrightMagenta": "#c0a7c7",
        "terminal.ansiBrightRed": "#d77c79",
        "terminal.ansiBrightWhite": "#ffffff",
        "terminal.ansiBrightYellow": "#f4cf87",
        "terminal.ansiCyan": "#9ac9c4",
        "terminal.ansiGreen": "#89cd7e",
        "terminal.ansiMagenta": "#c0a7c7",
        "terminal.ansiRed": "#d77c79",
        "terminal.ansiWhite": "#ffffff",
        "terminal.ansiYellow": "#f4cf86",
        "list.focusHighlightForeground": "#f1f9fa",
        "quickInputList.focusForeground": "#f1f9fa",
        "editorSuggestWidget.selectedForeground": "#f1f9fa",
        "list.focusHighlightBackground": "#47737d",
        "quickInputList.focusBackground": "#47737d",
        "editorSuggestWidget.selectedBackground": "#47737d",
    },
    "workbench.colorTheme": "Nord",
    "editor.semanticTokenColorCustomizations": {
        "[Nord]": {
            "rules": {
                "variable": "#d8dee9",
                "variable.definition": "#d8dee9",
                "variable.readonly": "#d08770",
                "namespace": "#8fbcbb",
                "type": "#81a1c1",
                "function": "#88c0d0",
                "operator": "#8fbcbb",
                "keyword": "#5e81ac"
                // "namespace.definition": "#eceff4",
            }
        }
    },
    // This colors are good for the Nord theme
    "bracket-pair-colorizer-2.colors": [
        "#eceff4",
        "#ebcb8b",
        "#b48ead",
        "#a3be8c",
        "#d08770"
    ],
    "solargraph.useBundler": true,
    "[ruby]": {
        "editor.defaultFormatter": "rebornix.ruby",
        "editor.tabSize": 2
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust"
    },
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "ruby.rubocop.useBundler": true,
    "editor.fontSize": 11,
    "editor.fontLigatures": true,
    "editor.folding": false,
    "editor.glyphMargin": false,
    "solargraph.checkGemVersion": false,
    "ruby.useLanguageServer": true,
    "markdown.extension.preview.autoShowPreviewToSide": true,
    "markdown.extension.toc.githubCompatibility": true,
    "editor.wrappingIndent": "indent",
    "files.trimFinalNewlines": true,
    "rust-client.enableMultiProjectSetup": true,
    "rust-client.updateOnStartup": true,
    "rust.all_features": true,
    "rust.clippy_preference": "on",
    "terraform.indexing": {
        "enabled": false,
        "liveIndexing": false
    },
    "terraform.languageServer": {
        "external": true,
        "args": [
            "serve"
        ]
    },
    "go.buildOnSave": "workspace",
    "[go]": {
        "editor.codeActionsOnSaveTimeout": 750,
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        },
        // Optional: Disable snippets, as they conflict with completion ranking.
        //"editor.snippetSuggestions": "none",
    },
    "go.addTags": {
        "options": "",
        "transform": "camelcase"
    },
    "[go.mod]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        },
    },
    "gopls": {
        // Add parameter placeholders when completing a function.
        "ui.completion.usePlaceholders": true,
        // If true, enable additional analyses with staticcheck.
        // Warning: This will significantly increase memory usage.
        "ui.diagnostic.staticcheck": true,
        // better syntax highlight
        "ui.semanticTokens": true,
        // run gofumpt (more strict than gofmt)
        "formatting.gofumpt": true,
        "formatting.local": "github.com/aws/modelrocket-cli",
        "ui.diagnostic.analyses": {
            // "fieldalignment": true,
            "nilness": true,
            "shadow": true,
            "unusedwrite": true
        }
    },
    "go.autocompleteUnimportedPackages": true,
    "go.delveConfig": {
        "dlvLoadConfig": {
            "followPointers": true,
            "maxVariableRecurse": 1,
            "maxStringLen": 64,
            "maxArrayValues": 64,
            "maxStructFields": -1
        },
        "apiVersion": 2,
        "showGlobalVariables": true
    },
    "editor.formatOnSave": true,
    "search.mode": "reuseEditor",
    "vs-kubernetes": {
        "vs-kubernetes.helm-path.mac": "/Users/guillermogaston/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vs-kubernetes.kubectl-path.mac": "/Users/guillermogaston/.vs-kubernetes/tools/kubectl/kubectl",
        "vs-kubernetes.minikube-path.mac": "/Users/guillermogaston/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vscode-kubernetes.minikube-path.mac": "/Users/guillermogaston/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "code-runner.enableAppInsights": false,
    "terminal.integrated.fontSize": 11,
    "launch": {


    
        "configurations": [],
        "compounds": []
    },
    "window.titleBarStyle": "custom",
    "editor.fontFamily": "'MesloLGS NF', Menlo, Monaco, 'Courier New', monospace",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "bashIde.highlightParsingErrors": true,
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.toolsManagement.autoUpdate": true,
    "python.showStartPage": false,
    "[json]": {
    
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.cursorStyle": "line",
    "editor.insertSpaces": false,
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["g", "i"],
            "commands": ["editor.action.goToImplementation"]
        }
    ],
    "cSpell.userWords": [
        "Customizer",
        "Distro",
        "EKSA",
        "Gomega",
        "Kube",
        "Kubectl",
        "Kustomize",
        "OIDC",
        "Wrapf",
        "eksd",
        "kubernetes",
        "retrier",
        "semver",
        "vsphere"
    ],
    "redhat.telemetry.enabled": false,
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    }
}